{
  "compilerOptions": {
    "target": "es2016",
    "lib": ["ES2016"],
    "useDefineForClassFields": true,
    "module": "commonjs",
    "noEmit": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
